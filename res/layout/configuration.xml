<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:weightSum="12">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_marginTop="15dp" android:layout_weight="2" android:gravity="center_vertical">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Impact sensitivity"
                android:id="@+id/textView"
                android:layout_marginLeft="20dp" android:textSize="18dp"/>
        <SeekBar
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/seekBar"/>
    </LinearLayout>
    <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp" android:layout_weight="4" android:weightSum="4">
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:weightSum="13" android:layout_weight="1">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView1" android:layout_weight="4" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Call"
                    android:id="@+id/textView2" android:layout_weight="3" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SMS"
                    android:id="@+id/textView3" android:layout_weight="3" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Post"
                    android:id="@+id/textView4" android:layout_weight="3" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
        </TableRow>
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:weightSum="13" android:layout_weight="1">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Impact"
                    android:id="@+id/textView5" android:layout_weight="4" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch1" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch2" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch3" android:layout_gravity="center_vertical"/>
            </FrameLayout>
        </TableRow>
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:weightSum="13" android:layout_weight="1">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Voice"
                    android:id="@+id/textView6" android:layout_weight="4" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch4" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch5" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch6" android:layout_gravity="center_vertical"/>
            </FrameLayout>
        </TableRow>
        <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" android:weightSum="13" android:layout_weight="1">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Button"
                    android:id="@+id/textView7" android:layout_weight="4" android:layout_gravity="center_vertical|left"
                    android:textSize="18dp"/>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch7" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/switch8" android:layout_gravity="center_vertical"/>
            </FrameLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" android:layout_weight="3">
                <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/ToggleButton9" android:layout_gravity="center_vertical"/>
            </FrameLayout>
        </TableRow>
    </TableLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="0dp" android:layout_weight="3">
    </LinearLayout>
</LinearLayout>